include "species.a"

const int NUM_POKEMON = 5;
PokemonSpecies[NUM_POKEMON] pBase;

static {
    pBase[0] = new PokemonSpecies("Charizard", 100, 100, 100, 100, 100, 100, PHYSICAL, "fighting", "none");
    pBase[1] = new PokemonSpecies("Pikachu", 100,100,100,100,100,100, SPECIAL, "ghost", "none");
    pBase[2] = new PokemonSpecies("Chikorita", 25, 25, 25, 25, 25, 25, SPECIAL, "grass", "None");
    pBase[3] = new PokemonSpecies("Squirtle", 10, 10, 10, 10, 10, 10, PHYSICAL, "fire", "fighting");
    pBase[4] = new PokemonSpecies("Tower", 10, 10, 10, 10, 10, 10, PHYSICAL, "fire", "fighting");
}

public PokemonSpecies speciesFromString(string a) {
    for (int i=0; i<NUM_POKEMON; ++i) {
        if (pBase[i].nam == a) {
            return pBase[i];
        }
    }
    return null;
}
