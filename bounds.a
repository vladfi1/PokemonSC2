import a.natives.region;

class MapBounds {
    public static region REGION;
    public static fixed WIDTH, HEIGHT;
    
    static {
        REGION = region.entireMap();
        point min = REGION.minBound();
        point max = REGION.maxBound();
        
        WIDTH = max.x - min.x;
        HEIGHT = max.y - min.y;
    }
}
